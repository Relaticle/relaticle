APP_NAME=Relaticle
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

# System Administrator Panel Configuration
# Use either SYSADMIN_DOMAIN for subdomain or SYSADMIN_PATH for path-based access
# SYSADMIN_DOMAIN=sysadmin.example.com
SYSADMIN_PATH=sysadmin

# Application Panel Configuration
# By default, the CRM panel is served at /app (path-based routing)
# For subdomain routing, set APP_PANEL_DOMAIN (e.g., app.example.com)
# APP_PANEL_DOMAIN=app.example.com
# APP_PANEL_PATH=app

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=relaticle
DB_USERNAME=postgres
DB_PASSWORD=postgres

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# Queue Configuration
# The application uses separate queues for different job types:
# - default: General application jobs
# - imports: CSV import jobs (handled by dedicated workers with higher timeout/memory)

CACHE_STORE=redis
CACHE_PREFIX=relaticle_cache

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

MAILCOACH_DOMAIN="relaticle.mailcoach.app"
MAILCOACH_API_TOKEN=""
MAILCOACH_SUBSCRIBERS_LIST_ID=""

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=

GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0

FATHOM_ANALYTICS_SITE_ID=

# Health Checks (opt-in)
# HEALTH_CHECKS_ENABLED=false
# OH_DEAR_HEALTH_CHECK_SECRET=

# Community Links
DISCORD_INVITE_URL=https://discord.gg/b9WxzUce4Q
