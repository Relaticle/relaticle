{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Next-Generation Open-Source CRM Platform",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "AGPL-3.0",
    "require": {
        "php": "^8.3",
        "ashallendesign/favicon-fetcher": "^3.8",
        "awcodes/overlook": "^v3.0",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "blade-ui-kit/blade-icons": "^1.8",
        "filament/filament": "^4.0",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.29",
        "laravel/jetstream": "^5.2",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.0",
        "relaticle/custom-fields": "@dev",
        "relaticle/flowforge": "@dev",
        "sentry/sentry-laravel": "^4.13",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/laravel-data": "^4.15",
        "spatie/laravel-login-link": "^1.3",
        "spatie/laravel-mailcoach-mailer": "^1.5",
        "spatie/laravel-mailcoach-sdk": "^1.4",
        "spatie/laravel-markdown": "^2.7",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-prefixed-ids": "^1.4",
        "spatie/laravel-settings": "^3.3",
        "spatie/laravel-sitemap": "^7.3",
        "symfony/http-client": "^7.2",
        "symfony/postmark-mailer": "^7.2",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "cweagans/composer-patches": "*",
        "fakerphp/faker": "^1.23",
        "itsgoingd/clockwork": "^5.3",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "rector/rector": "^2.0",
        "spatie/laravel-horizon-watcher": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Relaticle\\SystemAdmin\\": "app-modules/SystemAdmin/src",
            "Relaticle\\Documentation\\": "app-modules/Documentation/src",
            "Relaticle\\OnboardSeed\\": "app-modules/OnboardSeed/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "app-install": [
            "@composer install",
            "@php artisan relaticle:install"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "lint": "rector && pint --parallel",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse",
        "test:arch": "pest --filter=arch",
        "test:type-coverage": "pest --type-coverage --min=99.9",
        "test:coverage": "pest --coverage --min=80",
        "test:unit": "pest --parallel",
        "test:unit:shard": "pest --parallel --shard=${SHARD}",
        "test:unit:ci": "pest --parallel --configuration phpunit.ci.xml",
        "test:unit:ci:shard": "pest --parallel --configuration phpunit.ci.xml --shard=${SHARD}",
        "test": [
            "@test:lint",
            "@test:refactor",
            "@test:type-coverage",
            "@test:types",
            "@test:unit"
        ],
        "test:ci": [
            "@test:lint",
            "@test:refactor",
            "@test:type-coverage",
            "@test:types",
            "@test:unit:ci"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../custom-fields"
        },
        {
            "type": "path",
            "url": "../flowforge"
        }
    ]
}
